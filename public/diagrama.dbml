Table configuraciones{
    id int [pk, increment]/// Primary key
    nombre varchar 
    descripcion varchar
    direccion varchar
    telefono varchar
    correo_electronico varchar
    web varchar
    logo varchar
}

Table gestiones{
    id int [pk, increment] // Primary key
    nombre varchar
}


Table niveles{
    id int [pk, increment] //Primary key
    nombre varchar
} 

Table turnos{
    id int [pk, increment]//Primary key
    nombre varchar
}